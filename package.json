{
    "name": "quotes",
    "version": "0.0.0",
    "husky": {
        "hooks": {
            "pre-push": "yarn validate"
        }
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "jest",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "validate": "run-p test prettier:check lint:* prettier:check build:prod",
        "lint:ts": "ng lint",
        "lint:styles": "stylelint \"src/app/**/*.scss\"",
        "styles:format": "stylelint \"src/app/**/*.scss\" --fix",
        "prettier:check": "prettier --check  \"src/**/*.{ts,html}\"",
        "prettier:format": "prettier --write  \"src/**/*.{ts,html}\""
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.14",
        "@angular/common": "~8.2.14",
        "@angular/compiler": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "bulma": "^0.8.0",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.23",
        "@angular/cli": "~8.3.23",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^24.9.1",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "husky": "^4.2.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^25.1.0",
        "jest-preset-angular": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "protractor": "~5.4.0",
        "stylelint": "^13.0.0",
        "stylelint-config-standard": "^19.0.0",
        "stylelint-scss": "^3.13.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typescript": "~3.5.3"
    }
}
